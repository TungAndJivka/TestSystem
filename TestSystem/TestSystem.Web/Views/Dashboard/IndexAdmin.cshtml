
@{
    ViewData["Title"] = "IdexAdmin";
}

<h2>Admin Dashboard</h2>

<div>
    <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" id="tests-tab" data-toggle="tab" href="#tests" role="tab" aria-controls="tests" aria-selected="true">Test List</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="results-tab" data-toggle="tab" href="#results" role="tab" aria-controls="results" aria-selected="false">Test Results</a>
        </li>
    </ul>
    <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="tests" role="tabpanel" aria-labelledby="tests-tab">Tests</div>
        <div class="tab-pane fade" id="results" role="tabpanel" aria-labelledby="results-tab">Results</div>
    </div>
</div>

